{
  "name": "boomdraw/laravel-dummy-image",
  "description": "Laravel dummy image generator based on kingkool68/dummyimage",
  "keywords": [
    "boomdraw",
    "laravel",
    "laravel-dummy-image",
    "dummy",
    "image"
  ],
  "homepage": "https://github.com/boomdraw/laravel-dummy-image",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Slesarenko",
      "email": "info@boomdraw.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
  },
  "require-dev": {
    "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
    "phpunit/phpunit": "^6.5|^7.0"
  },
  "autoload": {
    "psr-4": {
      "BoomDraw\\DummyImage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BoomDraw\\DummyImage\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BoomDraw\\DummyImage\\DummyImageServiceProvider"
      ],
      "aliases": {
        "DummyImage": "BoomDraw\\DummyImage\\Facades\\DummyImage"
      }
    }
  }
}
